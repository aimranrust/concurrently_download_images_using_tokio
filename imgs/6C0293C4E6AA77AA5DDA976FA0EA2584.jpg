<script>
  document.addEventListener('DOMContentLoaded', function() {
    const addClass = (el, className) => el.classList.add(className);

    function initPoi(poiItem) {
      poiItem.addEventListener('click', function() {
        addClass(poiItem, 'poi__item--visited');

        const modalId = poiItem.querySelector('.poi__btn').getAttribute('aria-controls');
        const modal = document.getElementById(modalId);
        if (modal) {
          addClass(modal, 'active');
        }
      });
    }

    const initCloseButtons = () => {
      const closeButtons = document.querySelectorAll('.modal__close-btn');
      closeButtons.forEach(button => {
        button.addEventListener('click', function() {
          const modal = button.closest('.js-modal');
          if (modal) {
            modal.classList.remove('active');
          }
        });
      });
    };

    const poiItems = document.querySelectorAll('.js-poi__item');
    poiItems.forEach(initPoi);

    initCloseButtons();
  });
</script>




